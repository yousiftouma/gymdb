{
  "name": "GYmdb",
  "description": "Your ultimate movie database",
  "repository": "https://github.com/meanjs/mean",
  "logo": "/config/assets/img/logo/gymdb-logo.png",
  "keywords": [
    "mongodb",
    "express",
    "angularjs",
    "node.js",
    "mongoose",
    "passport"
  ],
  "addons": [
    {
      "plan": "mongolab:sandbox"
    },
    {
      "plan": "papertrail:choklad"
    }
  ],
  "env": {
    "NPM_CONFIG_PRODUCTION": "true",
    "NPM_CONFIG_LOGLEVEL": "error",
    "NODE_ENV": "production",
    "NODE_VERBOSE": "false"
  }
}
